#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,i,j;
        cin>>n;
        int c1=0;
        for(int i=2;i<=sqrt(n);i++)
        {
            int c=0;
            for(j=2;j<i;j++)
            {
                if(i%j==0)
                    c++;
            }
            if(c==0)
            {
                c1++;
            }

        }
    cout<<c1<<"\n";
}
}
