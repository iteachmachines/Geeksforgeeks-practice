#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t ;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        int count[n+1]={0};
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
            count[arr[i]]++;
        }
        for(int i=1;i<=n;i++)
            cout<<count[i]<<" ";
        cout<<"\n";

        }
    return 0;
    }
