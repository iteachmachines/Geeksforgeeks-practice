#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        cin>>arr[i];
        stack<int>s;
        s.push(0);
        cout<<"1";
        for(int i=1;i<n;i++)
        {
            while(!s.empty()&&arr[i]>=arr[s.top()])
                s.pop();
            if(s.empty())
                cout<<i+1<<" ";
            else
                cout<<i-s.top()<<" ";
            s.push(i);
        }
        cout<<"\n";
    }
    return 0;
}
