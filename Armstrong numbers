#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
    int ori,num,rem,sum=0;
    cin>>ori;
    num=ori;
    while(num!=0)
    {
        rem=num%10;
        sum+=rem*rem*rem;
        num/=10;
    }
    if(sum==ori)
        cout<<"Yes\n";
    else
        cout<<"No\n";
    }
    return 0;
}
